---
import BaseLayout from '../../../../layouts/BaseLayout.astro';

const lang = 'en';

const prospectTypes = [
  {
    type: 'Construction Contractors',
    problem: 'Need excavation for foundation, trenching',
    keywords: [
      'what size excavator for trenching foundation',
      'mini excavator vs big excavator for house construction',
      'buy or rent excavator for 3 month project',
      'can excavator fit through gate'
    ]
  },
  {
    type: 'Utility Contractors',
    problem: 'Need trenching for pipes, cables',
    keywords: [
      'how to dig trench for water pipe cheap',
      'excavator vs manual digging for trenching',
      'rent mini excavator for weekend project',
      'excavator rental for backyard'
    ]
  },
  {
    type: 'Landscaping Companies',
    problem: 'Site preparation, grading',
    keywords: [
      'how to level ground for lawn without manual labor',
      'mini excavator rental vs hiring landscapers',
      'excavator for garden pond digging',
      'rent excavator for landscaping project'
    ]
  },
  {
    type: 'Demolition Contractors',
    problem: 'Building teardown, site clearing',
    keywords: [
      'how to demolish small building safely',
      'excavator with breaker attachment rental',
      'rent excavator for demolition weekend',
      'mini excavator vs big excavator for demolition'
    ]
  },
  {
    type: 'Farm Owners',
    problem: 'Pond digging, land clearing',
    keywords: [
      'how to dig farm pond without expensive equipment',
      'rent excavator for farm use',
      'excavator rental for agricultural work',
      'mini excavator for small farm'
    ]
  },
  {
    type: 'DIY Homeowners',
    problem: 'Small projects, weekend work',
    keywords: [
      'can I rent excavator for DIY project',
      'how much to rent mini excavator for 1 day',
      'do I need license to operate mini excavator',
      'excavator rental for home improvement'
    ]
  },
  {
    type: 'Foreign Workers',
    problem: 'Language barrier, local supplier',
    keywords: [
      'how to find excavator supplier who speaks mandarin',
      'malay speaking excavator rental company near me',
      'excavator rental process malaysia for foreigners',
      'how to rent excavator in malaysia without experience'
    ]
  }
];

const languages = [
  { name: 'Malay', reason: 'CIDB contractors, small contractors, facility operators', reach: '60%' },
  { name: 'Chinese', reason: 'Own construction businesses, make procurement decisions', reach: '22%' },
  { name: 'English', reason: 'Corporate clients, MNCs, international projects', reach: '33%' }
];

const faq = [
  {
    q: 'How long until I see results?',
    a: 'Most clients see leads in 30 days. Some keywords rank faster. SEO grows over time.'
  },
  {
    q: 'What keywords do you target?',
    a: 'Problem-aware questions: "what size excavator for trenching", "excavator rental with operator", "cheap excavator rental KL". Also location searches: "excavator rental near me".'
  },
  {
    q: 'Do I need 3 languages?',
    a: 'Yes if you want government jobs (Malay) or Chinese business clients. English-only websites miss 2 out of 3 buyer groups.'
  },
  {
    q: 'Can I see examples?',
    a: 'Yes. We show you exactly what keywords we\'ll target. You approve before we start. No surprises.'
  }
];
