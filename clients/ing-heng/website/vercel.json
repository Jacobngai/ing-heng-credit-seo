{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "astro",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": false,
  "trailingSlash": true,
  "rewrites": [
    {
      "source": "/motor-insurance",
      "destination": "/en/services/insurance"
    },
    {
      "source": "/motor-insurance/",
      "destination": "/en/services/insurance/"
    },
    {
      "source": "/personal-insurance",
      "destination": "/en/services/insurance"
    },
    {
      "source": "/personal-insurance/",
      "destination": "/en/services/insurance/"
    }
  ],
  "redirects": [
    {
      "source": "/ms/:path*",
      "destination": "https://www.kreditloan.my/ms/:path*",
      "permanent": true
    },
    {
      "source": "/zh/:path*",
      "destination": "https://www.inghengcredit.my/zh/:path*",
      "permanent": true
    },
    {
      "source": "/immediate-edge-review-:slug*",
      "destination": "/410-gone",
      "statusCode": 410
    },
    {
      "source": "/category/foreks-brokery-2/:path*",
      "destination": "/410-gone",
      "statusCode": 410
    },
    {
      "source": "/tag/cos-play-porn/:path*",
      "destination": "/410-gone",
      "statusCode": 410
    },
    {
      "source": "/exploring-the-rise-of-onlyfans-:slug*",
      "destination": "/410-gone",
      "statusCode": 410
    },
    {
      "source": "/pocket-option-:slug*",
      "destination": "/410-gone",
      "statusCode": 410
    },
    {
      "source": "/bio-20-:slug*",
      "destination": "/410-gone",
      "statusCode": 410
    }
  ],
  "headers": [
    {
      "source": "/_vercel/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path(.*_g=.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    },
    {
      "source": "/((?!_vercel).*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    }
  ]
}
