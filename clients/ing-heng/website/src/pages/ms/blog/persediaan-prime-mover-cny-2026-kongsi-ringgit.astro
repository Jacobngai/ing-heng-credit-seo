---
import BaseLayout from '../../../layouts/BaseLayout.astro';
import Breadcrumbs from '../../../components/Breadcrumbs.astro';
import CTAButton from '../../../components/CTAButton.astro';
import { COMPANY_INFO } from '../../../utils/constants';

const lang = 'ms';
const post = {
  title: 'Persediaan Prime Mover CNY 2026 - Armada Sedia Untuk Kongsi Ringgit',
  slug: 'persediaan-prime-mover-cny-2026-kongsi-ringgit',
  excerpt: 'Dapatkan pembiayaan persediaan prime mover cny 2026 - armada sedia untuk kongsi ringgit dengan 0% down payment dari Ing Heng Credit. Kelulusan 24-48 jam, tiada had umur peralatan.',
  date: 'undefined',
  author: 'Ing Heng Credit Team',
  readTime: 3,
  category: 'Pembiayaan Prime Mover',
  image: 'undefined'
};

const keywords = [],
  breadcrumbItems = [
  { label: 'Laman Utama', href: '/ms' },
  { label: 'Blog', href: '/ms/blog' },
  { label: post.title, href: `/ms/blog/${post.slug}` }
