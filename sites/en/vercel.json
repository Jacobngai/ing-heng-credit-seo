{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "astro",
  "routes": [
    {
      "src": "/blog/2026-01-(.*)",
      "status": 301,
      "headers": { "Location": "/blog/$1" }
    },
    {
      "src": "/en/blog/2026-01-(.*)",
      "status": 301,
      "headers": { "Location": "/en/blog/$1" }
    },
    {
      "src": "/ms/(.*)",
      "status": 301,
      "headers": { "Location": "https://www.kreditloan.my/$1" }
    },
    {
      "src": "/zh/(.*)",
      "status": 301,
      "headers": { "Location": "https://www.inghengcredit.my/$1" }
    },
    {
      "src": "/.*\\?.*_g=.*",
      "status": 410,
      "headers": { "Cache-Control": "max-age=31536000" }
    }
  ]
}
