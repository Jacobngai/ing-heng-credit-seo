---
import BaseLayout from '../../../layouts/BaseLayout.astro';
import Breadcrumbs from '../../../components/Breadcrumbs.astro';
import CTAButton from '../../../components/CTAButton.astro';
import { COMPANY_INFO } from '../../../utils/constants';

const lang = 'en';
const post = {
  title: 'Lorry with Crane Financing Malaysia: Hiab, Palfinger & Self-Loader Trucks',
  slug: 'lorry-with-crane-financing-malaysia',
  excerpt: 'Lorry with crane financing Malaysia. Hiab, Palfinger, self-loader trucks, boom trucks. 0% down payment, used lorry financing, no age limit.',
  date: '2026-01-13',
  author: 'Ing Heng Credit Team',
  readTime: 8,
  category: 'Lorry Financing',
  image: '/images/blog/lorry-crane-financing.jpg'
};

const keywords = ['lorry with crane financing', 'Hiab truck', 'Palfinger crane', 'self loader lorry', 'boom truck financing', 'crane truck financing', '0% down payment'],
  breadcrumbItems = [
  { label: 'Home', href: '/en' },
  { label: 'Blog', href: '/en/blog' },
  { label: post.title, href: `/en/blog/${post.slug}` }
];
---

<BaseLayout title={post.title} description={post.excerpt} lang={lang} keywords={keywords}>
  <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "BlogPosting",
      "headline": post.title,
      "description": post.excerpt,
      "datePublished": post.date,
      "author": { "@type": "Organization", "name": post.author },
      "publisher": {
        "@type": "Organization",
        "name": "Ing Heng Credit & Leasing Sdn Bhd",
        "logo": {
          "@type": "ImageObject",
          "url": "https://inghengcredit.com/logo.png"
        }
      }
    }
  </script>

  <Breadcrumbs items={breadcrumbItems} />

  <article class="max-w-4xl mx-auto px-4 py-12">
    <header class="mb-8">
      <span class="text-sm font-semibold" style="color: #D4A574;">Lorry Financing</span>
      <h1 class="text-4xl font-bold mt-2" style="color: #2D1810; font-family: 'Bebas Neue', sans-serif; text-transform: uppercase;">{post.title}</h1>
