    color: #cd7f32;
    margin-bottom: 1rem;
  }

  .requirements-box ul {
    list-style: none;
    padding: 0;
  }

  .requirements-box li {
    padding: 0.5rem 0;
    padding-left: 1.5rem;
    position: relative;
  }

  .requirements-box li:before {
    content: "✓";
    position: absolute;
    left: 0;
    color: #cd7f32;
    font-weight: bold;
  }

  .process-timeline {
    display: grid;
    gap: 1.5rem;
    margin-top: 2rem;
  }

  .process-step {
    display: flex;
    align-items: flex-start;
    gap: 1.5rem;
    padding: 1.5rem;
    background: #f9f9f9;
    border-radius: 8px;
  }

  .step-number {
    background: #cd7f32;
    color: white;
    width: 50px;
    height: 50px;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 1.5rem;
    font-weight: bold;
    flex-shrink: 0;
  }

  .step-content h3 {
    color: #cd7f32;
    margin-bottom: 0.5rem;
  }

  .faq-item {
    background: #f9f9f9;
    padding: 1.5rem;
    margin-bottom: 1rem;
    border-radius: 8px;
    border-left: 3px solid #cd7f32;
  }

  .faq-item h3 {
    color: #1a1a1a;
    margin-bottom: 0.75rem;
  }

  .final-cta {
    background: linear-gradient(135deg, #cd7f32 0%, #a8652a 100%);
    color: white;
    padding: 3rem 2rem;
    border-radius: 8px;
    text-align: center;
    margin: 3rem 0;
  }

  .final-cta h2 {
    color: white;
  }

  .cta-buttons {
    display: flex;
    gap: 1rem;
    justify-content: center;
    margin: 2rem 0;
    flex-wrap: wrap;
  }

  .btn-primary,
  .btn-secondary {
    padding: 1rem 2rem;
    border-radius: 8px;
    text-decoration: none;
    font-weight: bold;
    display: inline-block;
  }

  .btn-primary {
    background: white;
    color: #cd7f32;
  }

  .btn-secondary {
    background: transparent;
    color: white;
    border: 2px solid white;
  }

  .contact-info {
    margin-top: 2rem;
    font-size: 1.1rem;
  }

  @media (max-width: 768px) {
    .hero-section h1 {
      font-size: 1.875rem;
    }

    .subtitle {
      font-size: 1rem;
    }

    .requirements-box {
      grid-template-columns: 1fr;
    }

    .process-step {
      flex-direction: column;
      text-align: center;
    }

    .step-number {
      margin: 0 auto;
    }
  }
</style>
